    int a[3][3],i,j,s;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    s=0;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
