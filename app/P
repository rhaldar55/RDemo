c implementation:
-----------------
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,a[100],s,i;
    scanf("%d",&n);
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    s=0;
    for(i=0;i<n;i++)
    {
        s=s+a[i];
        printf("%d ",s);
    }
    return 0;
}

java implementation:
--------------------
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int n=obj.nextInt();
        int i,s=0;
        int a[]=new int[n];
        for(i=0;i<n;i++)
            a[i]=obj.nextInt();
        for(i=0;i<n;i++)
        {
            s=s+a[i];
            System.out.print(s+" ");
        }
    }
}

