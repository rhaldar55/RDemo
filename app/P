        Scanner obj = new Scanner(System.in);
        int n=obj.nextInt();
        int d,c=0;
        while(n!=0)
        {
            d=n%10;
            if(d%2==0)
                c++;
            n=n/10;
        }
        System.out.println(c);
    }
}

