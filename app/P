print sum

c implementation:
-----------------
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n1,n2,i,sum;
    scanf("%d %d",&n1,&n2);
    sum=0;
    for(i=n1;i<=n2;i++){
        if(i%2==0){
            sum=sum+i;
        }
    }
    printf("%d",sum);
    return 0;
}

java implementation:
--------------------
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int n1=obj.nextInt();
        int n2=obj.nextInt();
        int i,sum;
        sum=0;
        for(i=n1;i<=n2;i++){
            if(i%2==0)
                sum=sum+i;
        }
        System.out.println(sum);
    }
}

