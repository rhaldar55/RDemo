#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[100];
    int i,c=0;
    scanf("%s",s);
    for(i=0;s[i];i++)
    {
        if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u')
            continue;
        else
            c++;
    }
    printf("%d",c);
    return 0;
}

