#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a[3][3],i,j,s;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<3;i++)
    {
	s=0;
        for(j=0;j<3;j++)
        {
	    s=s+a[j][i];
        }
	printf("%d\n",s);
    }
    return 0;
}

