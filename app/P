        int i,j,s;
        for(i=0;i<3;i++)
        {
            for(j=0;j<3;j++)
            {
                a[i][j]=obj.nextInt();
            }
        }
        s=1;
        for(i=0;i<3;i++)
        {
		s=s*a[i][3-i-1];
        }
        System.out.println(s);
    }
