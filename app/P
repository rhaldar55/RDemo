    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    for(i=0;i<n;i++)
    {
        if(i%2!=0)
            sum=sum+a[i];
    }
    printf("%d",sum);
    return 0;
}


java implementation:
--------------------
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int n=obj.nextInt();
        int a[]=new int[n];
        int i;
        int sum=0;
        for(i=0;i<n;i++)
            a[i]=obj.nextInt();
        for(i=0;i<n;i++) //i=i+2 or i+=2
        {
            if(i%2!=0)
                sum=sum+a[i];
        }
        System.out.println(sum);
    }
}

